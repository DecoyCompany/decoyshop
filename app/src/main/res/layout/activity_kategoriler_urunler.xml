<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.Kategoriler_urunler_Activity">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/urunler_liste"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@android:drawable/gallery_thumb"
        android:scrollbars="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.522"
        app:layout_constraintStart_toEndOf="@+id/Ust_kategoriler_liste"
        app:layout_constraintTop_toBottomOf="@+id/Alt_kategoriler_liste"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintWidth="match_constraint" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/Ust_kategoriler_liste"
        android:layout_width="wrap_content"
        android:layout_height="729dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/Alt_kategoriler_liste"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Ust_kategoriler_liste"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth="match_constraint" />
</androidx.constraintlayout.widget.ConstraintLayout>